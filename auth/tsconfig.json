{
  "extends": "../node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "allowJs": false,
    "outDir": "build",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports":true,
    "baseUrl": "./",                  
    "paths": {
      "auth":["src/auth"],
      "auth/*":["src/auth/*"],
      "auth/events":["src/events"],
      "auth/events/*":["src/events/*"]
    }
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts"
  ]
}
